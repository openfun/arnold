apiVersion: v1
kind: Endpoints
metadata:
  labels:
    app: edxapp
    endpoint: mongodb
  # name of the endpoint should be the same as the corresponding service
  name: mongodb
  namespace: "{{ project_name }}"
subsets:
  - addresses:
    - ip: {{ endpoint_mongodb_ip }}
    ports:
    - port: 27017
      name: 27017-tcp
